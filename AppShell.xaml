<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Academic_Tracker_Mobile_Development.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Academic_Tracker_Mobile_Development"
    xmlns:local2="clr-namespace:Academic_Tracker_Mobile_Development.Views"
    Title="Academic_Tracker_Mobile_Development"
    Shell.FlyoutBehavior="Flyout">


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Terms"
        ContentTemplate="{DataTemplate local2:Terms}"
        Route="Terms" />

    <ShellContent
        Title="Reports"
        ContentTemplate="{DataTemplate local2:Reports}"
        Route="Reports" />





</Shell>